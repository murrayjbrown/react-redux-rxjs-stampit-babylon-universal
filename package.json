{
  "name": "react-redux-rxjs-stampit-babylon-universal",
  "version": "0.1.0",
  "description": "A React/Redux/Rx demo app with BabylonJS game engine drivers using stamps.",
  "author": "Murray J Brown <mjb@mjbrown.com> (http://mjbrown.com)",
  "contributors": ["David Zukowski <david@zuko.me> (http://zuko.me)"],
  "license": "MIT",
  "private": true,
  "repository": {
    "private": true,
    "type": "git",
    "url": "git@github.com:murrayjbrown/react-redux-rxjs-stampit-babylon-universal.git"
  },
  "scripts": {
    "build": "better-npm-run build-dev",
    "build-dev": "better-npm-run build-dev",
    "build-prod": "better-npm-run build-prod",
    "build-test": "better-npm-run build-test",
    "commit": "git-cz",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" ",
    "lint": "eslint src tests",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "npm run build",
    "start": "concurrent --kill-others \"npm run start-dev\"",
    "start-dev": "better-npm-run start-dev",
    "start-prod": "better-npm-run start-prod",
    "start-test": "better-npm-run start-test",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "test:mocha": "mocha --debug-brk",
    "watch-client": "better-npm-run watch-client"
  },
  "pre-push": [
    "lint:fix",
    "lint",
    "test"
  ],
  "betterScripts": {
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-test": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "test",
        "PORT": 3000
      }
    },
    "build-dev": {
      "command": "webpack --verbose --colors --display-error-details --config config/webpack.dev.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build-prod": {
      "command": "webpack --verbose --colors --display-error-details --config config/webpack.prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-test": {
      "command": "webpack --verbose --colors --display-error-details --config config/webpack.test.config.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start karma.config.js",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "watch-client": {
      "command": "node config/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000
      }
    }
  },
  "engines": {
    "node": "^4.4.0",
    "npm": "^3.8.7"
  },
  "main": "bin/server.js",
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "babylonjs": "^2.4.0",
    "better-npm-run": "0.0.8",
    "bootstrap": "latest",
    "bootstrap-loader": "latest",
    "bootstrap-sass": "latest",
    "clean-webpack-plugin": "^0.1.9",
    "compression": "^1.6.0",
    "concurrently": "^0.1.1",
    "css-loader": "^0.23.1",
    "cssnano": "^3.3.2",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.4",
    "font-awesome": "^4.4.0",
    "font-awesome-webpack": "0.0.4",
    "fs-extra": "^0.30.0",
    "hand": "^0.1.2",
    "history": "^2.0.0",
    "html-webpack-plugin": "^2.7.1",
    "http-proxy": "^1.12.0",
    "imports-loader": "^0.6.5",
    "ip": "^1.1.2",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "node-sass": "^3.7.0",
    "parse": "^1.8.0",
    "parse-dashboard": "^1.0.13",
    "parse-server": "^2.2.11",
    "piping": "^0.3.0",
    "postcss-loader": "^0.9.0",
    "pretty-error": "^1.2.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-helmet": "^2.2.0",
    "react-hot-loader": "^3.0.0-beta.1",
    "react-redux": "^4.0.0",
    "react-router": "^2.2.0",
    "react-router-redux": "dlmr/react-router-redux#fix-inital-server-load-dist",
    "redux": "^3.0.0",
    "redux-observable": "^0.5.0",
    "resolve-url-loader": "latest",
    "rimraf": "^2.5.1",
    "rxjs": "^5.0.0-beta.8",
    "sass-loader": "^3.1.2",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "stampit": "^3.0.0-rc.9",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.14",
    "webpack-isomorphic-tools": "^2.2.18",
    "when": "^3.7.7",
    "yargs": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-typecheck": "^3.6.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "chai-enzyme": "^0.4.0",
    "cheerio": "^0.20.0",
    "codecov": "^1.0.1",
    "commitizen": "^2.8.1",
    "cz-customizable": "^4.0.0",
    "enzyme": "^2.0.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.2.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.1",
    "ghooks": "^1.2.1",
    "isparta-loader": "^2.0.0",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack-with-fast-source-maps": "^1.9.2",
    "mocha": "^2.2.5",
    "phantomjs-prebuilt": "^2.1.3",
    "pre-commit": "^1.1.3",
    "react-addons-test-utils": "^15.0.0",
    "react-transform-catch-errors": "^1.0.0",
    "redbox-react": "^1.2.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.6.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    },
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/cz-customizable.js"
    }
  }
}
